class baz{
	field name{__________}
	field email{__________}
	field info{__________}

	func exit{
		mov(eax,1)
		int(0x80)
	}

	func print(len,ptr){
		mov(ecx,ptr)
		mov(edx,len)
		mov(ebx,1)
		mov(eax,4)
		int(0x80)
	}
	
	func read(len,ptr):int{
		mov(esi,ptr)
		mov(edx,len)
		xor(eax)
		xor(edi)
		syscall
		mov(int,eax)
	}

	func main{
		loop{
			print(read(name.len,name),name)
		}
		exit
	}
}

