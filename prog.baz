field  prompt="  hello    enter name: "
field    name="............................................................"
field prompt2="  not a name: "
field     len=0
field   hello="hello\n"

func print(len:reg_rdx,ptr:reg_rcx){
    mov(rcx,ptr)
    mov(rdx,len)
    mov(rbx,1)
    mov(rax,4)
    int(0x80)
}

func inline exit(v){
    mov(rbx,v)
    mov(rax,1)
    int(0x80)
}

func inline main{
    var a=1
    print(hello.len,hello)
    exit(0)
}
