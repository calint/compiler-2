func exit(v){
    mov(ebx,v)
    mov(eax,1)
    int(0x80)
}

func print(len,ptr){
    mov(ecx,ptr)
    mov(edx,len)
    mov(ebx,1)
    mov(eax,4)
    int(0x80)
}

field hello{"hello world\n"}
field one{"one\n"}

func main{
    print(hello.len,hello)
    var a=1
    var b=2
    var c=3
    var d=4
    var n=10
    loop{
      if n=0 break
      var r=((a+b)*c+d)*2
      n=n-1
      if not n=1 continue
      print(one.len,one)
    }
    exit(0)
}
