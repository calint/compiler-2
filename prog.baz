func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline a(i):res{
    res=i+1
}

func inline b(i):res{
    res=i+2
}

func inline c(i):res{
    var a=1
    var b=2
    var c=a+b
    res=i+c
}

