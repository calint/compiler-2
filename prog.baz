func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline main{
    var a=1
    var b=2
    var c=3
    var d=4
    
    if a=0 or b=1 and c=3 or d=4 exit(1) # should be true
    if a=0 or (b=1 and c=3) or d=4 exit(2) # is true

    exit(0)
}
