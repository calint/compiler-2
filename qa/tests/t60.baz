func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline f(b:bool):res:bool{
    if b res=false else res=true
}

func inline main{
    var r:bool=f(true)
    if r exit(1)

    r=f(false)
    if not r exit(2)
    
    if 1=2 exit(3)
    
    if 1=1 r=true else r=false
    
    if not r exit(4)

    if 1=2 r=true else r=false
    
    if r exit(5)
    
    exit(0)
}
