func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline main{
    var a=4
    var b=2
    var c=a/b
    if not c==2 exit(1)

    a=3
    if not a%2==1 exit(2)

    c=1+3%2+1
    if not c==3 exit(3)

    c=-a/-b
    if not c==1 exit(4)

    c=-a/b
    if not c==-1 exit(5)

    exit(0)
}
