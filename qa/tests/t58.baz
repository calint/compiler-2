field hello="hello world"

func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline main{
    var bt1:i8=1
    var bt2:i8=2

    var b1:bool=bt1<bt2
    if not b1 exit(1)

    var b2:bool=bt1==1
    b2=not bt2
    if b2 exit(2)

    bt1=-1+2
    if not bt1==1 exit(3)

    exit(0)
}
