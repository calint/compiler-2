func inline exit(v:reg_rdi){
	mov(rax,60) # exit system call
	mov(rdi,v)  # return code
	syscall
}

func f1(b:byte,w:word,d:dword,q:qword):res{
	res=b+w+d+q
}

func inline f2(b:byte,w:word,d:dword,q:qword):res{
	res=b+w+d+q
}

func inline main{
	var b:byte=-1
	var w:word=-2
	var d:dword=-3
	var q:qword=-4

	var r=f1(b,w,d,q)
	if not r=-10 exit(1)
	
	r=f2(b,w,d,q)
	if not r=-10 exit(2)
	
	exit(0)
}
