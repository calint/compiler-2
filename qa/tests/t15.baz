field  hello="hello\n"

func print(len:reg_rdx,ptr:reg_rcx){
    mov(rcx,ptr)
    mov(rdx,len)
    mov(rbx,1)
    mov(rax,4)
    int(0x80)
}

func inline exit(v){
    mov(rbx,v)
    mov(rax,1)
    int(0x80)
}

func bar{
    print(hello.len,hello)
}

func inline main{
    var a=3
    var b=2
    loop{
        var c=1
        if a=0 break
        bar()
        a=a-1
    }
    exit(0)
}
