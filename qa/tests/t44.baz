func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func inline main{
    var a=2
    var b=~2
    var c=a&b
    if not c==0 exit(1)

    var d=a|b
    if not d==0xffffffffffffffff exit(2)
    
    var e=a
    var f=a^e
    if not f==0 exit(3)
    
    var g=1+2*a&2
    if not g==1 exit(4)

    var h=1
    var i=1
    var j=~h|i
    if not j==0xffffffffffffffff exit(5)

    var k=1+2*(h&1)
    if not k==3 exit(6)

    exit(0)
}
