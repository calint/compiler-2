func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

func f(a):res{
    res=-a*2
}

func inline main{
    var a=1
    var b=-f(-a)
    if(b==-2)
        exit(0)
    exit(1)
}
