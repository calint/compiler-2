func inline exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall
}

field len=-2

func inline foo(x:reg_rdx):i64 res{
    res=-x
}

func inline bar(x):i64 res{
    res=-x
}

func inline main{
    var a=-1
    var b=~-foo(-a)
    var c=~-(len)
    var d=~bar(len)
    if d==~-len and not a==b and a==-1 and b==~1 and c==~2
        exit(0)
    exit(1)
}
