func exit(v:reg_rdi){
    mov(rax,60) # exit system call
    mov(rdi,v)  # return code
    syscall()
}

func foo(x):i64 res{
    res=-x*2
}

func bar(y):i64 res{
    res=-y
}

func main(){
    var a=1
    var b=-foo(-bar(-a))
    if(b==-2)
        exit(0)
    exit(1)
}
